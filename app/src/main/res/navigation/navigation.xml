<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/mainScreen">


    <fragment
        android:id="@+id/mainScreen"
        android:name="uz.gita.dtm.presentation.ui.screen.fragment.main.MainScreen"
        android:label="MainScreen"
        tools:layout="@layout/screen_main" />
    <fragment
        android:id="@+id/loginScreen"
        android:name="uz.gita.dtm.presentation.ui.screen.fragment.auth.LoginScreen"
        android:label="screen_login"
        tools:layout="@layout/screen_login" />
    <fragment
        android:id="@+id/profileScreen2"
        android:name="uz.gita.dtm.presentation.ui.screen.fragment.main.profile.ProfileScreen"
        android:label="ProfileScreen" >
        <action
            android:id="@+id/action_profileScreen2_to_loginScreen"
            app:destination="@id/loginScreen" />
    </fragment>

</navigation>